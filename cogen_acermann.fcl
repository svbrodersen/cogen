ackerman(m, n):
0-ack-m-n: code := newHeader(list('ackerman, m, n));
        goto 1-ack-m-n;
2: return code;
1-ack-m-n: if isDone(list('ack, m, n), code) 2 else 3-ack-m-n;
3-ack-m-n: code := newBlock(code, list('ack, m, n));
        goto 4-ack-m-n;
4-ack-m-n: if (m = 0) 4-done-m-n else 4-ack1-m-n;
4-done-m-n: return o(code, list('return, list(list('quote, n), '+, 1)));
4-next-m-n: if (n = 0) 4-ack0-m-n else 4-ack1-m-n;
4-ack0-m-n: n := 1;
        m := (m - 1);
