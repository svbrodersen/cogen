        return o(code, list('return, list('quote, n)));
5-ack-m-n: if (m = 0) 5-done-m-n else 5-next-m-n;
5-done-m-n: return (n + 1);
5-next-m-n: if (n = 0) 5-ack0-m-n else 5-ack1-m-n;
5-ack0-m-n: n := 1;
        goto 5-ack2-m-n;
